{
  "name": "@jzone/sfc-playground-vant",
  "version": "1.0.15",
  "description": "Try Vant in the Playground. Currently only Vant 3+ is supported",
  "keywords": [
    "vant",
    "sfc",
    "playground",
    "repl"
  ],
  "homepage": "https://sfc-playground-vant.vercel.app/",
  "bugs": {
    "url": "https://github.com/zhixiaoqiang/sfc-playground-vant/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiaoqiang/sfc-playground-vant.git"
  },
  "license": "MIT",
  "author": "zhixiaoqiang <xdnazi1985@gmail.com> (https://zhixiaoqiang.github.io/zhixiaoqiang/)",
  "main": "index.html",
  "types": "global.d.ts",
  "scripts": {
    "build": "wireit",
    "check:all": "wireit",
    "check:js": "wireit",
    "check:markdownlint": "wireit",
    "check:sort-package-json": "wireit",
    "dev": "wireit",
    "preinstall": "wireit",
    "lint": "wireit",
    "markdownlint-fix": "wireit",
    "prepare": "wireit",
    "preview": "wireit",
    "release": "wireit",
    "serve": "wireit",
    "sort-package-json": "wireit"
  },
  "dependencies": {
    "@vue/repl": "^1.1.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.9.1",
    "vue": "3.2.37"
  },
  "devDependencies": {
    "@changesets/cli": "2.22.0",
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@types/file-saver": "2.0.5",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "@vitejs/plugin-vue": "2.3.3",
    "eslint": "8.17.0",
    "eslint-config-standard": "16.0.3",
    "eslint-module-utils": "2.7.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-vue": "8.7.1",
    "execa": "4.1.0",
    "husky": "7.0.4",
    "lint-staged": "12.5.0",
    "markdownlint-cli2": "0.4.0",
    "ora": "6.1.0",
    "prettier": "2.6.2",
    "sort-package-json": "1.57.0",
    "typescript": "4.7.3",
    "vite": "2.9.10",
    "vite-plugin-style-import": "1.4.1",
    "vue-eslint-parser": "8.3.0",
    "wireit": "0.5.0",
    "zx": "6.2.3"
  },
  "peerDependencies": {
    "vant": "^3.4.6"
  },
  "renovate": {
    "enabled": false
  },
  "wireit": {
    "build": {
      "command": "vite build"
    },
    "dev": {
      "command": "vite"
    },
    "preinstall": {
      "command": "node scripts/only-allow.mjs pnpm"
    },
    "prepare": {
      "command": "husky install"
    },
    "release": {
      "command": "zx scripts/release.mjs --npmPublish"
    },
    "serve": {
      "command": "vite preview"
    },
    "check:js": {
      "command": "eslint --ext .mjs,.ts,.js,.vue ./"
    },
    "check:markdownlint": {
      "command": "markdownlint-cli2 '**.md' '#node_modules'"
    },
    "check:sort-package-json": {
      "command": "sort-package-json --check"
    },
    "check:all": {
      "dependencies": [
        "check:js",
        "check:markdownlint",
        "check:sort-package-json"
      ]
    },
    "lint": {
      "command": "zx scripts/lint.mjs --all"
    },
    "markdownlint-fix": {
      "command": "markdownlint-cli2-fix '**.md' '#node_modules'"
    },
    "sort-package-json": {
      "command": "sort-package-json"
    }
  }
}
