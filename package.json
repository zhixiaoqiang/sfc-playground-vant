{
  "name": "@jzone/sfc-playground-vant",
  "version": "1.0.13",
  "description": "Try Vant in the Playground. Currently only Vant 3+ is supported",
  "keywords": [
    "vant",
    "sfc",
    "playground",
    "repl"
  ],
  "homepage": "https://sfc-playground-vant.vercel.app/",
  "bugs": {
    "url": "https://github.com/zhixiaoqiang/sfc-playground-vant/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiaoqiang/sfc-playground-vant.git"
  },
  "license": "MIT",
  "author": "zhixiaoqiang <xdnazi1985@gmail.com> (https://zhixiaoqiang.github.io/zhixiaoqiang/)",
  "main": "index.html",
  "types": "global.d.ts",
  "scripts": {
    "build": "vite build",
    "check:js": "eslint --ext .mjs,.ts,.js,.vue ./",
    "check:markdownlint": "markdownlint-cli2 '**.md' '#node_modules'",
    "check:sort-package-json": "sort-package-json --check",
    "dev": "vite",
    "preinstall": "node scripts/only-allow.mjs pnpm",
    "lint": "zx scripts/lint.mjs --all",
    "markdownlint-fix": "markdownlint-cli2-fix '**.md' '#node_modules'",
    "prepare": "husky install",
    "preview": "vite preview",
    "release": "zx scripts/release.mjs --npmPublish",
    "serve": "vite preview",
    "sort-package-json": "sort-package-json"
  },
  "dependencies": {
    "@vue/repl": "^1.0.0",
    "file-saver": "^2.0.5",
    "jszip": "^3.9.1",
    "vue": "3.2.33"
  },
  "devDependencies": {
    "@changesets/cli": "2.22.0",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@types/file-saver": "2.0.5",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "@vitejs/plugin-vue": "2.3.1",
    "eslint": "8.13.0",
    "eslint-config-standard": "16.0.3",
    "eslint-module-utils": "2.7.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-vue": "8.6.0",
    "execa": "4.1.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.8",
    "markdownlint-cli2": "0.4.0",
    "ora": "6.1.0",
    "prettier": "2.6.2",
    "sort-package-json": "1.55.0",
    "typescript": "4.6.3",
    "vite": "2.9.5",
    "vite-plugin-style-import": "1.4.1",
    "vue-eslint-parser": "8.3.0",
    "zx": "6.1.0"
  },
  "peerDependencies": {
    "vant": "^3.4.6"
  },
  "renovate": {
    "enabled": false
  }
}
