    "markdownlint-cli2": "^0.4.0",
    "ora": "^6.1.0",
    "prettier": "^2.6.2",
    "sort-package-json": "^1.55.0",
    "typescript": "^4.6.3",
    "vite": "^2.7.1",
    "vue-eslint-parser": "^8.3.0",
